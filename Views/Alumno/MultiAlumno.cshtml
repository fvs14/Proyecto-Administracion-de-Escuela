@model List<Alumno>

<strong>Lista de alumnos</strong>
<table class="table table-dark table-hover">
    <thead >
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Nombre</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var alumno in @Model)
    {
        <tr>
            <td>@alumno.Id</td>
            <td>@alumno.Nombre</td>
        </tr>
    }
    </tbody>
</table>



