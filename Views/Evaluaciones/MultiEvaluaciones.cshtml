@* @model List<Asignatura> *@
@model IEnumerable<Evaluaciones>


<body>
    
    <section class="main-section">
        <div class="caja">
            <strong>Lista de Evaluciones</strong>
            <table class="table table-dark table-hover">
                <thead >
                <tr>
                    <th scope="col">Id</th>
                    <th scope="col">Nombre</th>
                    <th scope="col">Jornada</th>
                </tr>
                </thead>
                <tbody>
                @foreach (var evaluaciones in Model)
                {
                    <tr>
                        <td>@evaluaciones.Id</td>
                        <td>@evaluaciones.Nombre</td>
                        <td>@evaluaciones.Nota</td>
                        
                        
                    </tr>
                }
                </tbody>
            </table>
        </div>
    </section>
    </div>
    
</body>